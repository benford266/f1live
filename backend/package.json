{
  "name": "f1-live-data-backend",
  "version": "1.0.0",
  "description": "Backend service for F1 Live Data Visualization",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "dev:no-redis": "node scripts/dev-no-redis.js",
    "dev:no-redis:watch": "nodemon scripts/dev-no-redis.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --testPathPattern=integration",
    "test:performance": "jest --testPathPattern=performance",
    "test:ci": "jest --coverage --ci --watchAll=false",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix"
  },
  "keywords": [
    "f1",
    "formula1",
    "signalr",
    "websocket",
    "realtime",
    "api"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "better-sqlite3": "^12.2.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-slow-down": "^3.0.0",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "isomorphic-dompurify": "^2.26.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "lz4": "^0.6.5",
    "node-fetch": "^2.7.0",
    "socket.io": "^4.7.5",
    "winston": "^3.11.0",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "eslint": "^8.56.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.1",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "jest-mock-extended": "^3.0.5",
    "nock": "^13.4.0",
    "nodemon": "^3.0.2",
    "redis-memory-server": "^0.10.0",
    "socket.io-client": "^4.7.5",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
